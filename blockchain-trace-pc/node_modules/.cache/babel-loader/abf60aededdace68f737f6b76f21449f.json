;\r\n    this.openLoading();\r\n  },\r\n  methods: {\r\n    /** 查询服务器信息 */\r\n    getList() {\r\n      getServer().then(response => {\r\n        this.server = response.data;\r\n        this.loading.close();\r\n      });\r\n    },\r\n    // 打开加载层\r\n    openLoading() {\r\n      this.loading = this.$loading({\r\n        lock: true,\r\n        text: \"拼命读取中\",\r\n        spinner: \"el-icon-loading\",\r\n        background: \"rgba(0, 0, 0, 0.7)\"\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>"],"mappings":";;;;;;AA8KA,IAAAA,OAAA,GAAAC,OAAA;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;eAEA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACA;MACAC,OAAA;MACA;MACAC,MAAA;IACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAC,OAAA;IACA,KAAAC,WAAA;EACA;EACAC,OAAA;IACA,cACAF,OAAA,WAAAA,QAAA;MAAA,IAAAG,KAAA;MACA,IAAAC,iBAAA,IAAAC,IAAA,WAAAC,QAAA;QACAH,KAAA,CAAAL,MAAA,GAAAQ,QAAA,CAAAV,IAAA;QACAO,KAAA,CAAAN,OAAA,CAAAU,KAAA;MACA;IACA;IACA;IACAN,WAAA,WAAAA,YAAA;MACA,KAAAJ,OAAA,QAAAW,QAAA;QACAC,IAAA;QACAC,IAAA;QACAC,OAAA;QACAC,UAAA;MACA;IACA;EACA;AACA;AAAAC,OAAA,CAAAC,OAAA,GAAAC,QAAA"}]}